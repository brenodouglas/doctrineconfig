{
    "name": "brenodouglas/doctrineconfig",
    "description": "Utility config doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "Breno Douglas",
            "email": "breno_douglas@hotmail.com"
        }
    ],
    "require": {
        "php" : ">=5.3",
        "composer/composer": "1.0.*@dev",
        "doctrine/common": "2.5.*",
        "doctrine/dbal": "2.5.*",
        "doctrine/orm": "2.5.*",
        "symfony/yaml": "2.*",
        "symfony/dependency-injection": "2.6.*@dev"
    },
    "autoload": {
	   "classmap": [
            "Doctrine",
            "Controller",
            "Hydrator",
            "Controller/Di"
      ]
    },

    "bin": ["bin/doctrine-config", "bin/doctrine-config.php"],

    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }


}
