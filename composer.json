{
    "name": "brenodouglas/doctrineconfig",
    "description": "Utility config doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "Breno Douglas",
            "email": "breno_douglas@hotmail.com"
        }
    ],
    "require": {
        "php" : ">=5.3",
        "composer/composer": "1.0.*@dev",
        "doctrine/common": "2.4.*",
        "doctrine/dbal": "2.4.1",
        "doctrine/orm": "2.4.1",
        "symfony/yaml": "2.*"
    },
    "autoload": {
	   "classmap": [
            "Doctrine",
            "Controller"
      ]
    },

    "bin": ["config/doctrine-config.php"],

    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }


}
